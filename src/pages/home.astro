---

const cookie:any = Astro.request.headers.get('cookie');
console.log(cookie);
if(!cookie.includes('accessToken') || !cookie.includes('refreshToken')) {
  console.log('redirecting to login');
  Astro.response.status = 302;
  Astro.response.headers.set('Location', '/login');
}

